{
    "name": "logicly/easy-oauth-client",
    "description": "Multi purpose OAuth2 client to use in Laravel",
    "license": "MIT",
    "homepage": "https://github.com/logiclybv/easy-oauth-client",
    "keywords": ["Laravel", "LaravelOAuthClient", "OAuth", "OAuthClient"],
    "require": {
        "illuminate/support": ">7",
        "php": "8.1"
    },
    "require-dev": {
        "phpunit/phpunit": ">9",
        "orchestra/testbench": ">5"
    },
    "autoload": {
        "psr-4": {
            "Logicly\\EasyOAuthClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Logicly\\EasyOAuthClient\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Logicly\\EasyOAuthClient\\EasyOAuthClientServiceProvider"
            ]
        }
    }
}
